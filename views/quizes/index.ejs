<form method="get" action="/quizes?search">
	<input type="text" name="search" placeholder="Buscador de preguntas;"/>
	<input type="submit" value="Enviar">
</form>

<table>
	<% var i; for (i=0; i < quizes.length; i++) { %>
		<tr><td><a href="quizes/<%= quizes[i].id %>"><%= quizes[i].pregunta %></a></td><td><a href="quizes/<%= quizes[i].id %>/edit"><button>editar</button></a></td></tr>
	<% } %>
</table>
<a href="/quizes/new"><button type="button">Crear pregunta</button></a>